<?xml version="1.0" encoding="utf-8"?>
<!-- 总代理注册经销商 -->
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="wrap_content">
    <TextView
        android:layout_marginTop="@dimen/common_dimension_25"
        android:text="注册完成"
        android:layout_gravity="center_horizontal"
        android:textColor="@color/green1"
        android:textSize="@dimen/LargeTextSize"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:layout_marginTop="@dimen/common_dimension_10"
        android:gravity="center_horizontal"
        android:text="欢迎您加入我们!以下是账号信息:"
        android:textColor="@color/black"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <TextView
        android:layout_marginTop="@dimen/common_dimension_10"
        android:textColor="@color/black"
        android:id="@+id/tvLoginName"
        android:text="登录名称:222"
        android:layout_gravity="center_horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:layout_marginBottom="@dimen/common_dimension_25"
        android:layout_marginTop="@dimen/common_dimension_10"
        android:text="初始密码:123456"
        android:textColor="@color/black"
        android:layout_gravity="center_horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</LinearLayout>